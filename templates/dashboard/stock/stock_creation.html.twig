{% extends 'base.html.twig' %}

{% block title %}
    {{ presenter.page.title.value }}
{% endblock %}

{% form_theme presenter.form 'form/themes/dashboard/_stock_creation.html.twig' %}

{% block body %}
    {{ form(presenter.form) }}
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    <script defer type="text/javascript" src="{{ asset('build/stockCreation.js') }}"></script>
    <script defer type="text/javascript" src="{{ asset('build/stock_tags.js') }}"></script>
    <script defer type="text/javascript" src="{{ asset('build/collectionHandler.js') }}"></script>
{% endblock %}
