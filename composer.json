{
    "type": "project",
    "name": "guikingone/marketReminder",
    "description": "The source code of the MarketReminder web/API application",
    "license": "proprietary",
    "require": {
        "php": "^7.0.8",
        "google/cloud-storage": "^1.3",
        "google/cloud-vision": "^0.9.1",
        "grpc/grpc": "^1.6",
        "lexik/jwt-authentication-bundle": "^2.4",
        "overblog/graphql-bundle": "^0.9.1",
        "predis/predis": "^1.1",
        "sensio/framework-extra-bundle": "^5.1",
        "sensiolabs/security-checker": "^4.1",
        "snc/redis-bundle": "^2.0",
        "symfony/asset": "^3.3",
        "symfony/console": "^3.3",
        "symfony/form": "^3.3",
        "symfony/framework-bundle": "^3.3",
        "symfony/intl": "^3.3",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "^1.0",
        "symfony/profiler-pack": "^1.0",
        "symfony/routing": "^3.3",
        "symfony/security-bundle": "^3.3",
        "symfony/serializer": "^3.3",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/translation": "^3.3",
        "symfony/twig-bundle": "^3.3",
        "symfony/validator": "^3.3",
        "symfony/webpack-encore-pack": "^1.0",
        "symfony/workflow": "^3.3",
        "symfony/yaml": "^3.3",
        "twig/extensions": "^1.5"
    },
    "require-dev": {
        "behat/gherkin": "^4.5",
        "behat/mink": "^1.7",
        "behat/mink-extension": "^2.3",
        "behat/mink-goutte-driver": "^1.2",
        "behat/mink-selenium2-driver": "^1.3",
        "behat/symfony2-extension": "^2.1",
        "behatch/contexts": "^3.0",
        "blackfire/php-sdk": "^1.11",
        "doctrine/doctrine-fixtures-bundle": "^2.4",
        "phpmetrics/phpmetrics": "^2.3",
        "phpspec/phpspec": "^4.3",
        "phpunit/phpunit": "^6.4",
        "symfony/dotenv": "^3.3",
        "symfony/flex": "^1.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "optimize-autoloader": true,
        "classmap-authoritative": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check": "script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*",
        "symfony/twig-bundle": "<3.3",
        "symfony/debug": "<3.3"
    },
    "extra": {
        "symfony": {
            "id": "01BWSSCH2XYWJ396713DCD2KSY",
            "allow-contrib": true
        }
    }
}
